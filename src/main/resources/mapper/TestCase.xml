<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.springboot.demo.mapper.TestCaseMapper">

    <resultMap id="TestCaseResultMap" type="TestCaseResults">

        <result column="planid" property="planid" />
        <result column="username" property="username" />
        <result column="buildversion" property="buildversion" />
        <result column="classname" property="classname" />
        <result column="methodname" property="methodname" />
        <result column="result" property="result" />
        <result column="exception" property="exception" />
        <result column="executiontime" property="executiontime" />
        <result column="message" property="message" />
        <result column="failure_log" property="failure_log" />

    </resultMap>

    <select id="getTestcases" resultMap="TestCaseResultMap">
        select * from testcase_info where planid=#{planid}
    </select>


</mapper>